GNU_ALWAYS_JSOURCES = \
  $(GNU_LISTS_JSOURCES) \
  $(GNU_TEXT_JSOURCES) \
  $(GNU_MAPPING_JSOURCES) \
  $(GNU_EXPR_JSOURCES) \
  $(GNU_KAWA_IO_JSOURCES) \
  $(GNU_KAWA_UTIL_JSOURCES) \
  $(GNU_KAWA_REFLECT_JSOURCES) \
  $(GNU_KAWA_FUNCTIONS_JSOURCES) \
  $(GNU_KAWA_LISPEXPR_JSOURCES) \
  $(GNU_KAWA_MODELS_JSOURCES) \
  $(GNU_ECMASCRIPT_JSOURCES)
gnu_java_sources = $(GNU_ALWAYS_JSOURCES)
if WITH_SWING
gnu_java_sources += $(GNU_KAWA_SWINGVIEWS_JSOURCES)
endif
if WITH_SWT
gnu_java_sources += $(GNU_KAWA_SWTVIEWS_JSOURCES)
endif
if WITH_JLINE2
gnu_java_sources += $(GNU_KAWA_IO_JLINE_JSOURCES)
endif
if ENABLE_XML
gnu_java_sources += $(GNU_XML_JSOURCES) $(GNU_KAWA_XML_JSOURCES) $(GNU_KAWA_LISPEXPR_XML_JSOURCES)
if WITH_SAX2
gnu_java_sources += $(GNU_KAWA_SAX_JSOURCES)
endif
endif
if ENABLE_ECHO2
gnu_java_sources += $(GNU_KAWA_ECHO2_JSOURCES)
endif
if ENABLE_XQUERY
gnu_java_sources += $(GNU_KAWA_XSLT_JSOURCES)
endif
if WITH_JAVAX_SCRIPT
gnu_java_sources += $(JAVAX_SCRIPT_EXTRA)
endif

dist_gnu_jsources = \
  $(GNU_ALWAYS_JSOURCES) \
  $(GNU_KAWA_SWINGVIEWS_JSOURCES) \
  $(GNU_KAWA_SWINGVIEWS_JSOURCES) \
  $(GNU_KAWA_SWTVIEWS_JSOURCES) \
  $(GNU_KAWA_IO_JLINE_JSOURCES) \
  $(GNU_XML_JSOURCES) \
  $(GNU_KAWA_XML_JSOURCES) \
  $(GNU_KAWA_LISPEXPR_XML_JSOURCES) \
  $(GNU_KAWA_SAX_JSOURCES) \
  $(GNU_KAWA_ECHO2_JSOURCES) \
  $(GNU_KAWA_XSLT_JSOURCES) \
  $(JAVAX_SCRIPT_EXTRA)

